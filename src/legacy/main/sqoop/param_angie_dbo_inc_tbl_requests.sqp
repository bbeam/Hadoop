--query
'select * from tblRequests where RequestDate >= TODATETIMEOFFSET('2016-01-01 00:00:00', '-05:00') and  RequestDate <= TODATETIMEOFFSET('2016-01-01 23:59:59', '-05:00') and $CONDITIONS'

--split-by
RequestID
--hive-delims-replacement
'#'
--fields-terminated-by
'\001' 
--lines-terminated-by
'\n'
--optionally-enclosed-by
'\"'
--null-string
'' 
--null-non-string
''